{
    "version": 2,
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "client/**",
                    "server/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}